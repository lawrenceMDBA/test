<?xml version="1.0" encoding="UTF-8"?>
<exportArchiveModule xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/exportArchiveModule.xsd">
	<!--Archive activities on areaId level. Export of simulated forecasts-->
	<!--FEWS simulations will be stored in -->
	<!--<archiveFolder>/<yyyy>/<MM>/<areaId>/<dd>/simulated/<workflowId><TimeZero><DispatchTime>-->
	<!--Within this directory the following sub-folders may exist: /timeseries, /reports, /modifiers, /states-->
	<exportSimulated>
		<general>
			<archiveFolder>$EXPORT_FOLDER_TO_ARCHIVE$</archiveFolder>
			<areaId>mdba</areaId>
			<idMapId>IdArchive</idMapId>
		</general>
		<activities>
			<netcdfExportActivities>
				<netcdfExportActivity>
					<fileName>URBS_$CATCHMENT$_Forecast.nc</fileName>
					<areaId>mdba</areaId>
					<sourceId>FloodOps</sourceId>
					<ncMetaData>
						<title>URBS official forecasts generated by ROWS-Flood</title>
						<institution>Murray-Darling Basin Authority</institution>
						<source>ROWS-Flood (Delft-FEWS)</source>
					</ncMetaData>
					<includeComments>true</includeComments>
					<timeSeriesSet>
						<moduleInstanceSetId>URBS_Rainfall</moduleInstanceSetId>
						<valueType>scalar</valueType>
						<parameterId>P_comb</parameterId>
						<locationSetId>URBS_subareas.$CATCHMENT$</locationSetId>
						<timeSeriesType>simulated forecasting</timeSeriesType>
						<timeStep unit="hour"/>
						<readWriteMode>read complete forecast</readWriteMode>
						<ensembleId>$ENSEMBLE$</ensembleId>
						<ensembleMemberId>$ENSMEMBER$</ensembleMemberId>
					</timeSeriesSet>
					<timeSeriesSet>
						<moduleInstanceSetId>URBS_Forecast</moduleInstanceSetId>
						<valueType>scalar</valueType>
						<parameterId>Q_fcst</parameterId>
						<locationSetId>URBS_fcst.$CATCHMENT$</locationSetId>
						<timeSeriesType>simulated forecasting</timeSeriesType>
						<timeStep unit="hour"/>
						<readWriteMode>read complete forecast</readWriteMode>
						<ensembleId>$ENSEMBLE$</ensembleId>
						<ensembleMemberId>$ENSMEMBER$</ensembleMemberId>
					</timeSeriesSet>
					<timeSeriesSet>
						<moduleInstanceSetId>URBS_Forecast</moduleInstanceSetId>
						<valueType>scalar</valueType>
						<parameterId>Q_in_fcst</parameterId>
						<locationSetId>URBS_reservoirs.$CATCHMENT$</locationSetId>
						<timeSeriesType>simulated forecasting</timeSeriesType>
						<timeStep unit="hour"/>
						<readWriteMode>read complete forecast</readWriteMode>
						<ensembleId>$ENSEMBLE$</ensembleId>
						<ensembleMemberId>$ENSMEMBER$</ensembleMemberId>
					</timeSeriesSet>
					<timeSeriesSet>
						<moduleInstanceSetId>URBS_Forecast</moduleInstanceSetId>
						<valueType>scalar</valueType>
						<parameterId>Q_out_fcst</parameterId>
						<locationSetId>URBS_reservoirs.$CATCHMENT$</locationSetId>
						<timeSeriesType>simulated forecasting</timeSeriesType>
						<timeStep unit="hour"/>
						<readWriteMode>read complete forecast</readWriteMode>
						<ensembleId>$ENSEMBLE$</ensembleId>
						<ensembleMemberId>$ENSMEMBER$</ensembleMemberId>
					</timeSeriesSet>
					<timeSeriesSet>
						<moduleInstanceSetId>URBS_Forecast</moduleInstanceSetId>
						<valueType>scalar</valueType>
						<parameterId>P_calc</parameterId>
						<qualifierId>gross</qualifierId>
						<locationSetId>URBS_fcst.$CATCHMENT$</locationSetId>
						<timeSeriesType>simulated forecasting</timeSeriesType>
						<timeStep unit="hour"/>
						<readWriteMode>read complete forecast</readWriteMode>
						<ensembleId>$ENSEMBLE$</ensembleId>
						<ensembleMemberId>$ENSMEMBER$</ensembleMemberId>
					</timeSeriesSet>
					<timeSeriesSet>
						<moduleInstanceSetId>URBS_Forecast</moduleInstanceSetId>
						<valueType>scalar</valueType>
						<parameterId>P_calc</parameterId>
						<qualifierId>net</qualifierId>
						<locationSetId>URBS_fcst.$CATCHMENT$</locationSetId>
						<timeSeriesType>simulated forecasting</timeSeriesType>
						<timeStep unit="hour"/>
						<readWriteMode>read complete forecast</readWriteMode>
						<ensembleId>$ENSEMBLE$</ensembleId>
						<ensembleMemberId>$ENSMEMBER$</ensembleMemberId>
					</timeSeriesSet>
					<timeSeriesSet>
						<moduleInstanceSetId>URBS_Forecast</moduleInstanceSetId>
						<valueType>scalar</valueType>
						<parameterId>H_fcst</parameterId>
						<locationSetId>URBS_reservoirs.$CATCHMENT$</locationSetId>
						<timeSeriesType>simulated forecasting</timeSeriesType>
						<timeStep unit="hour"/>
						<readWriteMode>read complete forecast</readWriteMode>
						<ensembleId>$ENSEMBLE$</ensembleId>
						<ensembleMemberId>$ENSMEMBER$</ensembleMemberId>
					</timeSeriesSet>
					<timeSeriesSet>
						<moduleInstanceSetId>URBS_QtoH_Forecast</moduleInstanceSetId>
						<valueType>scalar</valueType>
						<parameterId>H_fcst</parameterId>
						<locationSetId>URBS_rated.$CATCHMENT$</locationSetId>
						<timeSeriesType>simulated forecasting</timeSeriesType>
						<timeStep unit="hour"/>
						<readWriteMode>read complete forecast</readWriteMode>
						<ensembleId>$ENSEMBLE$</ensembleId>
						<ensembleMemberId>$ENSMEMBER$</ensembleMemberId>
					</timeSeriesSet>
					<timeSeriesSet>
						<moduleInstanceSetId>URBS_QtoH_Forecast</moduleInstanceSetId>
						<valueType>scalar</valueType>
						<parameterId>V_fcst</parameterId>
						<locationSetId>URBS_reservoirs.$CATCHMENT$</locationSetId>
						<timeSeriesType>simulated forecasting</timeSeriesType>
						<timeStep unit="hour"/>
						<readWriteMode>read complete forecast</readWriteMode>
						<ensembleId>$ENSEMBLE$</ensembleId>
						<ensembleMemberId>$ENSMEMBER$</ensembleMemberId>
					</timeSeriesSet>
				</netcdfExportActivity>
			</netcdfExportActivities>
		</activities>
	</exportSimulated>
</exportArchiveModule>