<?xml version="1.0" encoding="UTF-8"?>
<exportArchiveModule xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/exportArchiveModule.xsd">
	<!--FEWS ratings will be stored in <archiveFolder>/ratingcurves/<areaId>-->
	<!--Note: subsequent export activities will overwrite previous exports of rating curves -->
	<exportRatingCurves>
		<general>
			<archiveFolder>$EXPORT_FOLDER_TO_ARCHIVE$</archiveFolder>
			<includeModifiers>true</includeModifiers>
		</general>
		<activities>
			<ratingCurvesExportActivities>
				<ratingCurvesExportActivity>
					<fileName>ratings_ROWS.xml</fileName>
					<areaId>mdba</areaId>
					<locationSetId>sites_H_rated</locationSetId>
				</ratingCurvesExportActivity>
			</ratingCurvesExportActivities>
		</activities>
	</exportRatingCurves>
</exportArchiveModule>