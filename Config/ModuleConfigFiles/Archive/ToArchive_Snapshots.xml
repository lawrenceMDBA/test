<?xml version="1.0" encoding="UTF-8"?>
<exportArchiveModule xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/exportArchiveModule.xsd">
	<!--FEWS snapshots will be stored in <archiveFolder>/snapshots/<areaId>-->
	<!--Note: subsequent export activities will overwrite previous exports of rating curves -->
	<exportSnapShot>
		<general>
			<archiveFolder>$EXPORT_FOLDER_TO_ARCHIVE$</archiveFolder>
			<!--
 			<properties>
				<string key="" value=""></string>
			</properties>
			<dataSetName>dataSetName</dataSetName>
			 -->
			<zipExportedSnapShot>true</zipExportedSnapShot></general>
		<activities>
			<exportSnapShot>
				<areaId>mdba</areaId>
			</exportSnapShot>
		</activities>
	</exportSnapShot>
</exportArchiveModule>