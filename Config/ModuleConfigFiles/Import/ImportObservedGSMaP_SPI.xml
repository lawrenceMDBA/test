<?xml version="1.0" encoding="UTF-8"?>
<timeSeriesImportRun xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/timeSeriesImportRun.xsd">
	<import>
		<general>
			<importType>NETCDF-CF_GRID</importType>
			<folder>$IMPORT_FOLDER_FROM_JAXA$/SPI</folder>
			<fileNamePatternFilter>*.nc</fileNamePatternFilter>
			<fileNameObservationDateTimePattern>'SEMDP_SPI_GNRT6_0.25deg-MON_'yyyyMM'.nc'</fileNameObservationDateTimePattern>
			<failedFolder>$IMPORT_FAILED_FROM_JAXA$/SPI</failedFolder>
			<deleteImportedFiles>true</deleteImportedFiles>
			<logWarningsForUnmappableTimeSeries>true</logWarningsForUnmappableTimeSeries>
			<idMapId>IdGSMaP</idMapId>
			<importTimeZone>
				<timeZoneOffset>+00:00</timeZoneOffset>
			</importTimeZone>
			<!-- Need to confirm geodatum -->
			<geoDatum>WGS 1984</geoDatum>
			<dataFeedId>GSMaP</dataFeedId>
			<synchLevel>6</synchLevel>
			<expiryTime unit="day" multiplier="365"/>
		</general>
<!-- 		<timeSeriesSet>
			<moduleInstanceId>ImportGSMaP</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>P_obs</parameterId>
			<locationId>GSMaP_Pricip</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep multiplier="1" unit="hour"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet> -->
		<timeSeriesSet>
			<moduleInstanceId>ImportGSMaP</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>SPI_calc</parameterId>
			<qualifierId>monthly</qualifierId>
			<locationId>GSMaP_SPI</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep multiplier="1" unit="month"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>ImportGSMaP</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>SPI_calc</parameterId>
			<qualifierId>2monthly</qualifierId>
			<locationId>GSMaP_SPI</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep multiplier="1" unit="month"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>ImportGSMaP</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>SPI_calc</parameterId>
			<qualifierId>3monthly</qualifierId>
			<locationId>GSMaP_SPI</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep multiplier="1" unit="month"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
		<externUnit cumulativeSum="true" parameterId="P_obs" unit="mm"/>
	</import>
</timeSeriesImportRun>