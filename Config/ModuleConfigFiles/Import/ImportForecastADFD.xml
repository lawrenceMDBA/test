<?xml version="1.0" encoding="UTF-8"?>
<timeSeriesImportRun xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/timeSeriesImportRun.xsd">
	<import>
		<general>
			<importType>NETCDF-CF_GRID</importType>
			<folder>$IMPORT_FOLDER_FROM_BOM_ADFD$</folder>
			<failedFolder>$IMPORT_FAILED_FROM_BOM$</failedFolder>
			<deleteImportedFiles>true</deleteImportedFiles>
			<logWarningsForUnmappableTimeSeries>true</logWarningsForUnmappableTimeSeries>
			<idMapId>IdADFD</idMapId>
			<importTimeZone>
				<timeZoneOffset>+00:00</timeZoneOffset>
			</importTimeZone>
			<geoDatum>WGS 1984</geoDatum>
			<dataFeedId>BOM_ADFD</dataFeedId>
			<synchLevel>6</synchLevel>
			<expiryTime unit="day" multiplier="2"/>
		</general>
		<timeSeriesSet>
			<moduleInstanceId>ImportForecastADFD</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>P_fcst</parameterId>
			<locationId>ADFD</locationId>
			<timeSeriesType>external forecasting</timeSeriesType>
			<timeStep unit="hour" multiplier="3"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>ImportForecastADFD</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>P_fcst</parameterId>
			<qualifierId>higher</qualifierId>
			<locationId>ADFD</locationId>
			<timeSeriesType>external forecasting</timeSeriesType>
			<timeStep id="1am"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>ImportForecastADFD</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>AT_fcst</parameterId>
			<qualifierId>max</qualifierId>
			<locationId>ADFD</locationId>
			<timeSeriesType>external forecasting</timeSeriesType>
			<timeStep unit="nonequidistant"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>ImportForecastADFD</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>AT_fcst</parameterId>
			<qualifierId>min</qualifierId>
			<locationId>ADFD</locationId>
			<timeSeriesType>external forecasting</timeSeriesType>
			<timeStep unit="nonequidistant"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>ImportForecastADFD</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>AT_fcst</parameterId>
			<locationId>ADFD</locationId>
			<timeSeriesType>external forecasting</timeSeriesType>
			<timeStep unit="hour"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
	</import>
</timeSeriesImportRun>