<?xml version="1.0" encoding="UTF-8"?>
<timeSeriesImportRun xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/timeSeriesImportRun.xsd">
	<!-- imports forecast data from MDBC_OPS as simulated forecasting with 10 day expiry time-->
	<import>
		<general>
			<importType>csv</importType>
			<folder>$IMPORT_FOLDER_FROM_MDBC_OPS$</folder>
			<fileNamePatternFilter>*forecast*</fileNamePatternFilter>
			<failedFolder>$IMPORT_FAILED_FROM_MDBC_OPS$</failedFolder>
			<logWarningsForUnmappableTimeSeries>true</logWarningsForUnmappableTimeSeries>
			<logWarningsForUnmappableLocations>true</logWarningsForUnmappableLocations>
			<logWarningsForUnmappableParameters>true</logWarningsForUnmappableParameters>
			<idMapId>IdmapMDBC_OPS2ROWS_Forecast</idMapId>
			<unitConversionsId>ImportUnitConversions</unitConversionsId>
			<missingValue>-9</missingValue>
			<importTimeZone>
				<timeZoneOffset>+10:00</timeZoneOffset>
			</importTimeZone>
			<dataFeedId>MDBC_OPS</dataFeedId>
			<expiryTime unit="month" multiplier="18"/>
		</general>
		<timeSeriesSet>
			<moduleInstanceId>ImportMDBC_OPS</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>H_fcst</parameterId>
			<locationSetId>sites</locationSetId>
			<timeSeriesType>external forecasting</timeSeriesType>
			<timeStep id="8am"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>ImportMDBC_OPS</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>P_fcst</parameterId>
