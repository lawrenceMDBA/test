			<parameterId>H_obs</parameterId>
			<locationSetId>threshold_sites_dH_hourly</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="hour"/>
			<relativeViewPeriod end="0" start="-367" startOverrulable="true" unit="day"/>
