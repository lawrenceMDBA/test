	<transformation id="Doctors simulated observed 10hrs">
		<user>
			<simple>
				<!--<expression>if (Heywoods_observed &lt;=  60000, (Doctors_observed - Heywoods_shift_10hrs + Hume_Release_shift_10hrs), Heywoods_observed)</expression>-->
				<expression>if (Heywoods_observed_Q_raw &gt; 70000, Doctors_observed_Q_raw - Heywoods_raw_shift_3hrs + Heywoods_simulated_shift_3hrs, if (Heywoods_observed_Q_raw &gt; 60000, Doctors_observed_Q_raw - Heywoods_raw_shift_7hrs + Heywoods_simulated_shift_7hrs, if(Heywoods_observed_Q_raw &lt;=  60000, Doctors_observed_Q_raw - Heywoods_raw_shift_10hrs + Heywoods_simulated_shift_10hrs, Heywoods_observed_Q_raw)))</expression>
				<outputVariable>
					<variableId>Doctors_simulated_flow</variableId>
				</outputVariable>
			</simple>
		</user>
	</transformation>
