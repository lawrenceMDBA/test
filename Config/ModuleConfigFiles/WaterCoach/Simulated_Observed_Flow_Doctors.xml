			</constant>
		</timeShift>
	</transformation>
	<transformation id="Albury Simulated Flow">
		<user>
			<simple>
				<expression>if (Doctors_simulated_flow &lt;= 70000, Doctors_simulated_shift_3hrs, if (Doctors_simulated_flow &gt; 70000, Doctors_simulated_shift_1hr, Doctors_simulated_flow))</expression>
				<outputVariable>
					<variableId>Albury_simulated_flow</variableId>
				</outputVariable>
			</simple>
		</user>
	</transformation>
	<transformation id="Albury simulated level">
	   	<dischargeStage>
			<ratingCurve>
				<discharge>
					<variableId>Albury_simulated_flow</variableId>
				</discharge>
				<stage>
					<variableId>Albury_simulated_level</variableId>
				</stage>
			</ratingCurve>
		</dischargeStage>
	</transformation>
	<transformation id="Albury Simulated H">
	   <merge>
		  <simple>
			 <inputVariable>
				<variableId>Albury_simulated_level</variableId>
			 </inputVariable>
			 <outputVariable>
				<variableId>Albury_simulated_level_nonequi</variableId>
			 </outputVariable>
		  </simple>
	   </merge>
	</transformation>
</transformationModule>
