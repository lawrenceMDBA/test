	<transformation id="StageToDischarge_Doctors">
		<stageDischarge>
			<ratingCurve>
				<stage>
					<variableId>Doctors_observed_H_raw</variableId>
				</stage>
				<discharge>
					<variableId>Doctors_observed_Q_raw</variableId>
				</discharge>
			</ratingCurve>
		</stageDischarge>
	</transformation>
