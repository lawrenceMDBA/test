	<!--Process rainfall data is based on the Rainfall scenario selection-->
	<!--Step 1: Compute sub-area averages from all rainfall forecast products-->
	<!--Step 2: Disaggregate all original NWP subarea data to 1 hour time intervals-->
	<!--Step 3: Select NWP rainfall source-->
	<!--Step 4: Interpolate Gauges Rainfall to subareas-->
	<!--Step 5: Generate Manual Forecaster Series and select correct Manual scenario-->
	<!--Step 6: Merge all series together and use backup of 0 mm-->
	<!--********************************************************-->
	<!--************ Start of Transformations *************-->
	<!--********************************************************-->
	<!--Step 1: Compute sub-area averages from all rainfall forecast products-->
	<transformation id="Interpolate_ACCESSG_to_UrbsSubAreas">
		<interpolationSpatial>
			<closestDistance>
				<inputVariable>
					<variableId>ACCESSG_P_grid</variableId>
				</inputVariable>
				<searchRadius>100000</searchRadius>
				<distanceGeoDatum>UTM55S</distanceGeoDatum>
				<outputVariable>
					<variableId>ACCESSG_P_UrbsSubAreas</variableId>
				</outputVariable>
			</closestDistance>
		</interpolationSpatial>
	</transformation>
	<transformation id="Interpolate_ADFD_to_UrbsSubAreas">
		<interpolationSpatial>
			<closestDistance>
				<inputVariable>
					<variableId>ADFD_P_grid</variableId>
				</inputVariable>
				<searchRadius>100000</searchRadius>
				<distanceGeoDatum>UTM55S</distanceGeoDatum>
				<outputVariable>
					<variableId>ADFD_P_UrbsSubAreas</variableId>
				</outputVariable>
			</closestDistance>
		</interpolationSpatial>
	</transformation>
	<transformation id="Interpolate_ADFD_higher_to_UrbsSubAreas">
		<interpolationSpatial>
			<closestDistance>
				<inputVariable>
					<variableId>ADFD_higher_P_grid</variableId>
				</inputVariable>
				<searchRadius>100000</searchRadius>
				<distanceGeoDatum>UTM55S</distanceGeoDatum>
				<outputVariable>
					<variableId>ADFD_higher_P_UrbsSubAreas</variableId>
				</outputVariable>
			</closestDistance>
		</interpolationSpatial>
	</transformation>
	<transformation id="Interpolate_PME_to_UrbsSubAreas">
		<interpolationSpatial>
			<closestDistance>
				<inputVariable>
					<variableId>PME_P_grid</variableId>
				</inputVariable>
				<searchRadius>100000</searchRadius>
				<distanceGeoDatum>UTM55S</distanceGeoDatum>
				<outputVariable>
					<variableId>PME_P_UrbsSubAreas</variableId>
				</outputVariable>
			</closestDistance>
		</interpolationSpatial>
	</transformation>
	<transformation id="Interpolate_PME_min_to_UrbsSubAreas">
		<interpolationSpatial>
			<closestDistance>
				<inputVariable>
					<variableId>PME_min_P_grid</variableId>
				</inputVariable>
				<searchRadius>100000</searchRadius>
				<distanceGeoDatum>UTM55S</distanceGeoDatum>
				<outputVariable>
					<variableId>PME_min_P_UrbsSubAreas</variableId>
				</outputVariable>
			</closestDistance>
		</interpolationSpatial>
	</transformation>
	<transformation id="Interpolate_PME_max_to_UrbsSubAreas">
		<interpolationSpatial>
			<closestDistance>
				<inputVariable>
					<variableId>PME_max_P_grid</variableId>
				</inputVariable>
				<searchRadius>100000</searchRadius>
				<distanceGeoDatum>UTM55S</distanceGeoDatum>
				<outputVariable>
					<variableId>PME_max_P_UrbsSubAreas</variableId>
				</outputVariable>
			</closestDistance>
		</interpolationSpatial>
	</transformation>
	<!--Step 2: Disaggregate all original NWP subarea data to hourly intervals-->
	<transformation id="ACCESSG">
		<disaggregation>
			<accumulative>
				<inputVariable>
					<variableId>ACCESSG_P_UrbsSubAreas</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>ACCESSG</variableId>
				</outputVariable>
			</accumulative>
		</disaggregation>
	</transformation>
	<transformation id="ADFD">
		<disaggregation>
			<accumulative>
				<inputVariable>
					<variableId>ADFD_P_UrbsSubAreas</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>ADFD</variableId>
				</outputVariable>
			</accumulative>
		</disaggregation>
	</transformation>
	<transformation id="ADFD_higher">
		<disaggregation>
			<accumulative>
				<inputVariable>
					<variableId>ADFD_higher_P_UrbsSubAreas</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>ADFD_higher</variableId>
				</outputVariable>
			</accumulative>
		</disaggregation>
	</transformation>
	<transformation id="PME">
		<disaggregation>
			<accumulative>
				<inputVariable>
					<variableId>PME_P_UrbsSubAreas</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>PME</variableId>
				</outputVariable>
			</accumulative>
		</disaggregation>
	</transformation>
	<transformation id="PME_min">
		<disaggregation>
			<accumulative>
				<inputVariable>
					<variableId>PME_min_P_UrbsSubAreas</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>PME_min</variableId>
				</outputVariable>
			</accumulative>
		</disaggregation>
	</transformation>
	<transformation id="PME_max">
		<disaggregation>
			<accumulative>
				<inputVariable>
					<variableId>PME_max_P_UrbsSubAreas</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>PME_max</variableId>
				</outputVariable>
			</accumulative>
		</disaggregation>
	</transformation>
	<!--Step 3: Select NWP Source and multiply-->
	<transformation id="Select_NWP">
		<merge>
			<selectDataSource>
				<input>
					<variableId>ACCESSG</variableId>
				</input>
				<input>
					<variableId>ADFD</variableId>
				</input>
				<input>
					<variableId>ADFD_higher</variableId>
				</input>
				<input>
					<variableId>PME</variableId>
				</input>
				<input>
					<variableId>PME_min</variableId>
				</input>
				<input>
					<variableId>PME_max</variableId>
				</input>
				<input>
					<variableId>None</variableId>
				</input>
				<attributeFunctionId>@catchment:CATCH_P_NWP_SELECT@</attributeFunctionId>
				<output>
					<variableId>NWP_subareas</variableId>
				</output>
			</selectDataSource>
		</merge>
	</transformation>
	<transformation id="Multiply_NWP">
		<user>
			<simple>
				<expression>NWP_subareas * multiplier</expression>
				<coefficientSetFunctions>
					<coefficient id="multiplier" value="@catchment:CATCH_P_NWP_MULT@"/>
				</coefficientSetFunctions>
				<outputVariable>
					<variableId>NWP_subareas</variableId>
				</outputVariable>
			</simple>
		</user>
	</transformation>
	<transformation id="Select_NWP_Scenario">
		<merge>
			<selectDataSource>
				<input>
					<variableId>ACCESSG</variableId>
				</input>
				<input>
					<variableId>ADFD</variableId>
				</input>
				<input>
					<variableId>ADFD_higher</variableId>
				</input>
				<input>
					<variableId>PME</variableId>
				</input>
				<input>
					<variableId>PME_min</variableId>
				</input>
				<input>
					<variableId>PME_max</variableId>
				</input>
				<input>
					<variableId>None</variableId>
				</input>
				<attributeFunctionId>@catchment:SCEN_CATCH_P_NWP_SELECT@</attributeFunctionId>
				<output>
					<variableId>NWP_subareas_Scenario</variableId>
				</output>
			</selectDataSource>
		</merge>
	</transformation>
	<transformation id="Multiply_NWP_Scenario">
		<user>
			<simple>
				<expression>NWP_subareas_Scenario * multiplier</expression>
				<coefficientSetFunctions>
					<coefficient id="multiplier" value="@catchment:SCEN_CATCH_P_NWP_MULT@"/>
				</coefficientSetFunctions>
				<outputVariable>
					<variableId>NWP_subareas_Scenario</variableId>
				</outputVariable>
			</simple>
		</user>
	</transformation>
	<!--Step 4: Interpolate Gauges Rainfall to subareas-->
	<transformation id="P_gapfill_1h">
		<interpolationSerial>
			<default>
				<inputVariable>
					<variableId>P_1h</variableId>
				</inputVariable>
				<defaultValue>0</defaultValue>
				<outputVariable>
					<variableId>P_1h_tmp</variableId>
				</outputVariable>
			</default>
		</interpolationSerial>
	</transformation>
	<transformation id="InverseDistance_P_sites_to_subareas">
		<interpolationSpatial>
			<inverseDistance>
				<inputVariable>
					<variableId>P_1h_tmp</variableId>
				</inputVariable>
				<minimumValue>0</minimumValue>
				<searchRadius>100000</searchRadius>
				<distanceGeoDatum>UTM55S</distanceGeoDatum>
				<inverseDistancePower>2</inverseDistancePower>
				<numberOfPoints>4</numberOfPoints>
				<numberOfBackupPoints>8</numberOfBackupPoints>
				<outputVariable>
					<variableId>Gauges</variableId>
				</outputVariable>
			</inverseDistance>
		</interpolationSpatial>
	</transformation>
	<!--Step 5: Generate Manual Forecaster Series-->
	<transformation id="Manual_Depth_Forecast_SubCatch">
		<generation>
			<constant>
				<coefficientSetFunctions>
					<duration unit="@catchment:CATCH_P_UNIT@" multiplier="@subcatchment:SC_P_MAN_LENGTH@"/>
					<totalSum>@subcatchment:SC_P_MAN_DEPTH@</totalSum>
					<delay unit="@catchment:CATCH_P_UNIT@" multiplier="@subcatchment:SC_P_MAN_DELAY@"/>
					<offset unit="@catchment:CATCH_P_UNIT@" value="@subcatchment:SC_P_MAN_OFFSET@"/>
				</coefficientSetFunctions>
				<outputVariable>
					<variableId>Depth</variableId>
				</outputVariable>
			</constant>
		</generation>
	</transformation>
	<transformation id="Manual_Recession_Forecast_SubCatch">
		<generation>
			<recessionMean>
				<inputVariable>
					<variableId>Gauges</variableId>
				</inputVariable>
				<coefficientSetFunctions>
					<duration unit="@catchment:CATCH_P_UNIT@" multiplier="@subcatchment:SC_P_MAN_LENGTH@"/>
					<matchingPeriod unit="@catchment:CATCH_P_UNIT@" multiplier="@subcatchment:SC_P_MAN_MATCH@"/>
					<recessionFactor>@subcatchment:SC_P_MAN_FACTOR@</recessionFactor>
					<offset unit="@catchment:CATCH_P_UNIT@" value="@subcatchment:SC_P_MAN_OFFSET@"/>
				</coefficientSetFunctions>
				<outputVariable>
					<variableId>Recession</variableId>
				</outputVariable>
			</recessionMean>
		</generation>
	</transformation>
	<transformation id="Manual_Persistence_Forecast_SubCatch">
		<generation>
			<recessionPattern>
				<inputVariable>
					<variableId>Gauges</variableId>
				</inputVariable>
				<coefficientSetFunctions>
					<duration unit="@catchment:CATCH_P_UNIT@" multiplier="@subcatchment:SC_P_MAN_LENGTH@"/>
					<matchingPeriod unit="@catchment:CATCH_P_UNIT@" multiplier="@subcatchment:SC_P_MAN_MATCH@"/>
					<recessionFactor>@subcatchment:SC_P_MAN_FACTOR@</recessionFactor>
					<offset unit="@catchment:CATCH_P_UNIT@" value="@subcatchment:SC_P_MAN_OFFSET@"/>
				</coefficientSetFunctions>
				<outputVariable>
					<variableId>Persistence</variableId>
				</outputVariable>
			</recessionPattern>
		</generation>
	</transformation>
	<transformation id="Select_Nowcast_SubCatch">
		<merge>
			<selectDataSource>
				<input>
					<variableId>Depth</variableId>
				</input>
				<input>
					<variableId>Recession</variableId>
				</input>
				<input>
					<variableId>Persistence</variableId>
				</input>
				<input>
					<variableId>None</variableId>
				</input>
				<attributeFunctionId>@subcatchment:SC_P_MAN_SELECT@</attributeFunctionId>
				<output>
					<variableId>Manual_subareas</variableId>
				</output>
			</selectDataSource>
		</merge>
	</transformation>
	<transformation id="Manual_Depth_Forecast_SubCatch_Scenario">
		<generation>
			<constant>
				<coefficientSetFunctions>
					<duration unit="@catchment:CATCH_P_UNIT@" multiplier="@subcatchment:SCEN_SC_P_MAN_LENGTH@"/>
					<totalSum>@subcatchment:SCEN_SC_P_MAN_DEPTH@</totalSum>
					<delay unit="@catchment:CATCH_P_UNIT@" multiplier="@subcatchment:SCEN_SC_P_MAN_DELAY@"/>
					<offset unit="@catchment:CATCH_P_UNIT@" value="@subcatchment:SCEN_SC_P_MAN_OFFSET@"/>
				</coefficientSetFunctions>
				<outputVariable>
					<variableId>Depth</variableId>
				</outputVariable>
			</constant>
		</generation>
	</transformation>
	<transformation id="Manual_Recession_Forecast_SubCatch_Scenario">
		<generation>
			<recessionMean>
				<inputVariable>
					<variableId>Gauges</variableId>
				</inputVariable>
				<coefficientSetFunctions>
					<duration unit="@catchment:CATCH_P_UNIT@" multiplier="@subcatchment:SCEN_SC_P_MAN_LENGTH@"/>
					<matchingPeriod unit="@catchment:CATCH_P_UNIT@" multiplier="@subcatchment:SCEN_SC_P_MAN_MATCH@"/>
					<recessionFactor>@subcatchment:SCEN_SC_P_MAN_FACTOR@</recessionFactor>
					<offset unit="@catchment:CATCH_P_UNIT@" value="@subcatchment:SCEN_SC_P_MAN_OFFSET@"/>
				</coefficientSetFunctions>
				<outputVariable>
					<variableId>Recession</variableId>
				</outputVariable>
			</recessionMean>
		</generation>
	</transformation>
	<transformation id="Manual_Persistence_Forecast_SubCatch_Scenario">
		<generation>
			<recessionPattern>
				<inputVariable>
					<variableId>Gauges</variableId>
				</inputVariable>
				<coefficientSetFunctions>
					<duration unit="@catchment:CATCH_P_UNIT@" multiplier="@subcatchment:SCEN_SC_P_MAN_LENGTH@"/>
					<matchingPeriod unit="@catchment:CATCH_P_UNIT@" multiplier="@subcatchment:SCEN_SC_P_MAN_MATCH@"/>
					<recessionFactor>@subcatchment:SCEN_SC_P_MAN_FACTOR@</recessionFactor>
					<offset unit="@catchment:CATCH_P_UNIT@" value="@subcatchment:SCEN_SC_P_MAN_OFFSET@"/>
				</coefficientSetFunctions>
				<outputVariable>
					<variableId>Persistence</variableId>
				</outputVariable>
			</recessionPattern>
		</generation>
	</transformation>
	<transformation id="Select_Nowcast_SubCatch_Scenario">
		<merge>
			<selectDataSource>
				<input>
					<variableId>Depth</variableId>
				</input>
				<input>
					<variableId>Recession</variableId>
				</input>
				<input>
					<variableId>Persistence</variableId>
				</input>
				<input>
					<variableId>None</variableId>
				</input>
				<attributeFunctionId>@subcatchment:SCEN_SC_P_MAN_SELECT@</attributeFunctionId>
				<output>
					<variableId>Manual_subareas_Scenario</variableId>
				</output>
			</selectDataSource>
		</merge>
	</transformation>
	<!--Processing steps for final product: merge observed with NWP products to create merged $ENSEMBLE$s-->
	<transformation id="Merge Observed and ACCESS_G: NWP $ENSEMBLE$ 1">
		<merge>
			<simple>
				<inputVariable>
					<variableId>Gauges</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>ACCESSG</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_1</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<transformation id="Fill_gaps_with_zero_Ens_1">
		<interpolationSerial>
			<default>
				<inputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_1</variableId>
				</inputVariable>
				<defaultValue>0</defaultValue>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_1</variableId>
				</outputVariable>
			</default>
		</interpolationSerial>
	</transformation>
	<transformation id="Merge Observed and ADFD: NWP $ENSEMBLE$ 3">
		<merge>
			<simple>
				<inputVariable>
					<variableId>Gauges</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>ADFD</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_3</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<transformation id="Fill_gaps_with_zero_Ens_3">
		<interpolationSerial>
			<default>
				<inputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_3</variableId>
				</inputVariable>
				<defaultValue>0</defaultValue>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_3</variableId>
				</outputVariable>
			</default>
		</interpolationSerial>
	</transformation>
	<transformation id="Merge Observed and ADFD_higher: NWP $ENSEMBLE$ 4">
		<merge>
			<simple>
				<inputVariable>
					<variableId>Gauges</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>ADFD_higher</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_4</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<transformation id="Fill_gaps_with_zero_Ens_4">
		<interpolationSerial>
			<default>
				<inputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_4</variableId>
				</inputVariable>
				<defaultValue>0</defaultValue>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_4</variableId>
				</outputVariable>
			</default>
		</interpolationSerial>
	</transformation>
	<transformation id="Merge Observed and PME: NWP $ENSEMBLE$ 5">
		<merge>
			<simple>
				<inputVariable>
					<variableId>Gauges</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>PME</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_5</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<transformation id="Fill_gaps_with_zero_Ens_5">
		<interpolationSerial>
			<default>
				<inputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_5</variableId>
				</inputVariable>
				<defaultValue>0</defaultValue>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_5</variableId>
				</outputVariable>
			</default>
		</interpolationSerial>
	</transformation>
	<transformation id="Merge Observed and PME_min: NWP $ENSEMBLE$ 6">
		<merge>
			<simple>
				<inputVariable>
					<variableId>Gauges</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>PME_min</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_6</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<transformation id="Fill_gaps_with_zero_Ens_6">
		<interpolationSerial>
			<default>
				<inputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_6</variableId>
				</inputVariable>
				<defaultValue>0</defaultValue>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_6</variableId>
				</outputVariable>
			</default>
		</interpolationSerial>
	</transformation>
	<transformation id="Merge Observed and PME_max: NWP $ENSEMBLE$ 7">
		<merge>
			<simple>
				<inputVariable>
					<variableId>Gauges</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>PME_max</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_7</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<transformation id="Fill_gaps_with_zero_Ens_7">
		<interpolationSerial>
			<default>
				<inputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_7</variableId>
				</inputVariable>
				<defaultValue>0</defaultValue>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_7</variableId>
				</outputVariable>
			</default>
		</interpolationSerial>
	</transformation>
	<transformation id="Merge Observed and Manual Official Forecast: NWP $ENSEMBLE$ 8">
		<merge>
			<simple>
				<inputVariable>
					<variableId>Gauges</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>Manual_subareas</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>NWP_subareas</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_8</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<transformation id="Fill_gaps_with_zero_Ens_8">
		<interpolationSerial>
			<default>
				<inputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_8</variableId>
				</inputVariable>
				<defaultValue>0</defaultValue>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_8</variableId>
				</outputVariable>
			</default>
		</interpolationSerial>
	</transformation>
	<transformation id="Merge Observed and Manual Scenario Forecast: NWP $ENSEMBLE$ 9">
		<merge>
			<simple>
				<inputVariable>
					<variableId>Gauges</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>Manual_subareas_Scenario</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>NWP_subareas_Scenario</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_9</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<transformation id="Fill_gaps_with_zero_Ens_9">
		<interpolationSerial>
			<default>
				<inputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_9</variableId>
				</inputVariable>
				<defaultValue>0</defaultValue>
				<outputVariable>
					<variableId>Subareas_Merged_Forecast_ENS_9</variableId>
				</outputVariable>
			</default>
		</interpolationSerial>
	</transformation>
</transformationModule>