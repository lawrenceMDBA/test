				</timeSeriesSet>
			</line>
			<line>
				<axis>left</axis>
				<timeSeriesSet>
					<moduleInstanceId>ProcessStorages</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>V_obs</parameterId>
