					<relativeViewPeriod end="0" start="-14" startOverrulable="true" unit="day"/>
					<readWriteMode>add originals</readWriteMode>
				</timeSeriesSet>
			</line> 
			<line>
				<axis>left</axis>
				<timeSeriesSet>
					<moduleInstanceId>ProcessStorages</moduleInstanceId>
