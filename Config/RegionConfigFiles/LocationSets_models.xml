	<locationSet id="URBS_subareas">
		<esriShapeFile>
			<file>upperMurray_urbs_subareas</file>
			<geoDatum>GDA94</geoDatum>
			<id>%SUBCATCHID%_%SUBAREAID%</id>
			<name>%SUBCATCHID%_%SUBAREAID%</name>
			<description>%SUBCATCHM% (%CATCHMENT%)</description>
			<relation id="subcatchment">
				<relatedLocationId>%SUBCATCHID%</relatedLocationId>
			</relation>
			<relation id="catchment">
				<relatedLocationId>%CATCHMENT%</relatedLocationId>
			</relation>
			<attribute id="URBS_SUBAREAID" name="SUBAREAID">
				<description>Numeric identifier of sub area within sub catchment</description>
				<text>%SUBAREAID%</text>
			</attribute>
			<attribute id="URBS_SUBCATCHMENT_ID" name="URBS sub-catchment Id">
				<description>URBS catchment id</description>
				<text>%SUBCATCHID%</text>
			</attribute>
			<attribute id="URBS_SUBCATCHMENT" name="URBS sub-catchment">
				<description>URBS catchment</description>
				<text>%SUBCATCHM%</text>
			</attribute>
			<attribute id="URBS_CATCHMENT" name="URBS catchment">
				<description>URBS catchment</description>
				<text>%CATCHMENT%</text>
			</attribute>
			<attribute id="URBS_INPUTPAR_PMERGED" name="URBS_inputPar_Pmerged">
				<description>Fixed urbs parameter name</description>
				<text>P.obs</text>
			</attribute>
		</esriShapeFile>
	</locationSet>
	<locationSet id="URBS_subcatchments.upper_murray">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>URBS_subcatchments</locationSetId>
		<constraints>
			<attributeTextEquals id="URBS_CATCHMENT" equals="upper_murray"/>
		</constraints>
	</locationSet>
	<locationSet id="URBS_subareas.upper_murray">
		<locationSetId>URBS_subareas</locationSetId>
		<constraints>
			<attributeTextContains id="URBS_CATCHMENT" contains="upper_murray"/>
		</constraints>
	</locationSet>
	<locationSet id="URBS_subareas.upper_murray_upper_m">
