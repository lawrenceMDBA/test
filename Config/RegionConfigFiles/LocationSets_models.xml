<?xml version="1.0" encoding="UTF-8"?>
<locationSets version="1.1" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/locationSets.xsd">
	<!-- ewsource  RO-->
	<!-- P stand for Planning and O stands for Operational (opo) data -->
	<locationSet id="ewSourceRO_sites">
		<description>Sites to be exported to ewSourceRO</description>
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>sites</locationSetId>
		<constraints>
			<attributeExists id="EWSRC_ID"/>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_PQ">
		<description>ewSourceRO_sites with flow</description>
		<sortingLocationAttributeId>EWSRC_Q_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="Q;"/>
				<attributeTextContains id="EWSRC_Q_SEQ" contains="PQ"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OQ">
		<description>ewSourceRO_sites with flow</description>
		<sortingLocationAttributeId>EWSRC_Q_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="Q;"/>
				<attributeTextContains id="EWSRC_Q_SEQ" contains="OQ"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OQORD">
		<description>ewSourceRO_sites with orders or demand</description>
		<sortingLocationAttributeId>EWSRC_QORD_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="QORD;"/>
				<attributeTextContains id="EWSRC_QORD_SEQ" contains="OQORD"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OQORD.longterm">
		<description>ewSourceRO_sites with flow</description>
		<sortingLocationAttributeId>EWSRC_QORD_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="QORD.longterm;"/>
				<attributeTextContains id="EWSRC_QORD_SEQ" contains="OQORD"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OQORD.mdbc">
		<description>ewSourceRO_sites with flow</description>
		<sortingLocationAttributeId>EWSRC_QORD_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="QORD.mdbc;"/>
				<attributeTextContains id="EWSRC_QORD_SEQ" contains="OQORD"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OQ.turb">
		<description>ewSourceRO_sites with turbine flow</description>
		<sortingLocationAttributeId>EWSRC_Q_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="Q.turb"/>
				<attributeTextContains id="EWSRC_Q_SEQ" contains="OQ"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OQ.valves">
		<description>ewSourceRO_sites with flow through valves</description>
		<sortingLocationAttributeId>EWSRC_Q_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="Q.valves"/>
				<attributeTextContains id="EWSRC_Q_SEQ" contains="OQ"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OQRL">
		<description>ewSourceRO_sites with releases</description>
		<sortingLocationAttributeId>EWSRC_Q_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="QRL"/>
				<attributeTextContains id="EWSRC_Q_SEQ" contains="OQ"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OQI">
		<description>ewSourceRO_sites with Inflow</description>
		<sortingLocationAttributeId>EWSRC_Q_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="Q.inflow;"/>
				<attributeTextContains id="EWSRC_Q_SEQ" contains="OQ"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OEGEN">
		<description>ewSourceRO_sites with electricity generation </description>
		<sortingLocationAttributeId>EWSRC_Q_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="EGEN"/>
				<attributeTextContains id="EWSRC_Q_SEQ" contains="OQ"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OH">
		<description>ewSourceRO_sites with level</description>
		<sortingLocationAttributeId>EWSRC_H_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="H;"/>
				<attributeTextContains id="EWSRC_H_SEQ" contains="OH"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OV">
		<description>ewSourceRO_sites with volume</description>
		<sortingLocationAttributeId>EWSRC_V_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="V;"/>
				<attributeTextContains id="EWSRC_V_SEQ" contains="OV"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OP">
		<description>ewSourceRO_sites with rainfall</description>
		<sortingLocationAttributeId>EWSRC_P_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="P;"/>
				<attributeTextContains id="EWSRC_P_SEQ" contains="OP"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OE">
		<description>ewSourceRO_sites with evaporation</description>
		<sortingLocationAttributeId>EWSRC_E_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="EVAP;"/>
				<attributeTextContains id="EWSRC_E_SEQ" contains="OE"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OE.fao56">
		<description>ewSourceRO_sites with evaporation (fao56)</description>
		<sortingLocationAttributeId>EWSRC_E_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="EVAP.fao56;"/>
				<attributeTextContains id="EWSRC_E_SEQ" contains="OE"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OE.mlake">
		<description>ewSourceRO_sites with evaporation (morton lake)</description>
		<sortingLocationAttributeId>EWSRC_E_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="EVAP.mlake;"/>
				<attributeTextContains id="EWSRC_E_SEQ" contains="OE"/>
			</allValid>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRO_sites_OEC">
		<description>ewSourceRO_sites with electrical conductivity </description>
		<sortingLocationAttributeId>EWSRC_EC_SEQ</sortingLocationAttributeId>
		<locationSetId>ewSourceRO_sites</locationSetId>
		<constraints>
			<allValid>
				<attributeTextContains id="DATA_ENTRY_PARAMS" contains="EC;"/>
				<attributeTextContains id="EWSRC_EC_SEQ" contains="OEC"/>
			</allValid>
		</constraints>
	</locationSet>
	<!-- ewsource  RR Model-->
	<locationSet id="ewsource_subcatchments">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<esriShapeFile>
			<file>ovens_ewSource_subcatchments</file>
			<projectionFileAvailable>true</projectionFileAvailable>
			<id>%CATCHMENTI%</id>
			<name>%CATCHMENTI%</name>
			<description>Ovens ewSource subcatchments</description>
			<shortName>%CATCHMENTI%</shortName>
		</esriShapeFile>
	</locationSet>
	<locationSet id="ewSourceRR_sites">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>sites</locationSetId>
		<constraints>
			<attributeExists id="EWSRC_ID"/>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRR_sites_Qrated">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>ewSourceRR_sites</locationSetId>
		<constraints>
			<attributeExists id="RATING"/>
		</constraints>
	</locationSet>
	<locationSet id="ewSourceRR_sites_Qobs">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>ewSourceRR_sites</locationSetId>
		<constraints>
			<not>
				<attributeTextEquals equals="Y" id="Q_OBS"/>
			</not>
		</constraints>
	</locationSet>
	<!-- STFmodel sites-->
	<locationSet id="stf_sites">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>sites</locationSetId>
		<constraints>
			<attributeTextContains id="MODEL_TYPE" contains="STF"/>
		</constraints>
	</locationSet>
	<!-- TSM sites-->
	<locationSet id="tsm_sites">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>sites</locationSetId>
		<constraints>
			<attributeTextContains id="MODEL_TYPE" contains="TSM"/>
		</constraints>
	</locationSet>
	<locationSet id="tsm_sites_AT">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>sites</locationSetId>
		<constraints>
			<attributeTextContains id="MODEL_TYPE" contains="TSM_AT"/>
		</constraints>
	</locationSet>
	<locationSet id="tsm_sites_E">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>sites</locationSetId>
		<constraints>
			<attributeTextContains id="MODEL_TYPE" contains="TSM_E"/>
		</constraints>
	</locationSet>
	<locationSet id="tsm_sites_P">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>sites</locationSetId>
		<constraints>
			<attributeTextContains id="MODEL_TYPE" contains="TSM_P"/>
		</constraints>
	</locationSet>
	<locationSet id="tsm_sites_H">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>sites</locationSetId>
		<constraints>
			<attributeTextContains id="MODEL_TYPE" contains="TSM_H"/>
		</constraints>
	</locationSet>
	<locationSet id="tsm_sites_Gates">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>sites</locationSetId>
		<constraints>
			<attributeTextContains id="MODEL_TYPE" contains="TSM_GO"/>
		</constraints>
	</locationSet>
	<locationSet id="tsm_sites_Q_obs">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>sites</locationSetId>
		<constraints>
			<attributeTextEquals id="Q_OBS" equals="Y"/>
			<attributeTextContains id="MODEL_TYPE" contains="TSM_Q"/>
		</constraints>
	</locationSet>
	<locationSet id="tsm_sites_Q_rated">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>sites</locationSetId>
		<constraints>
			<attributeTextContains id="MODEL_TYPE" contains="TSM_Q"/>
			<attributeExists id="RATING"/>
			<not>
				<attributeTextEquals id="Q_OBS" equals="Y"/>
			</not>
		</constraints>
	</locationSet>
	<locationSet id="tsm_sites_output">
		<sortingLocationAttributeId>HYDSTRA_SITE_ID</sortingLocationAttributeId>
		<locationSetId>sites</locationSetId>
		<constraints>
			<attributeTextContains id="MODEL_TYPE" contains="TSM"/>
		</constraints>
	</locationSet>
	<!--URBS-->
	<locationSet id="URBS_catchments">
		<csvFile>
			<file>ROWS_Rainfall_Catchments.csv</file>
			<geoDatum>GDA94</geoDatum>
			<id>%CATCHMENT%</id>
			<name>%CATCHMENT%</name>
			<description>%CATCHMENT%</description>
			<x>0</x>
			<y>0</y>
			<relation id="catchment">
				<relatedLocationId>%CATCHMENT%</relatedLocationId>
			</relation>
			<attribute id="CATCHMENT" name="Catchment">
				<description>Catchment</description>
				<text>%CATCHMENT%</text>
			</attribute>
			<attribute id="CATCH_P_UNIT" name="Unit">
				<description>Seelct Catchment or Subcatchment</description>
				<text>%P_UNIT%</text>
			</attribute>
			<attribute id="CATCH_P_NWP_SELECT" name="Source">
				<description>NWP Rainfall Source</description>
				<text>%P_NWP_SELECT%</text>
			</attribute>
			<attribute id="CATCH_P_NWP_MULT" name="Multiplier">
				<description>NWP Rainfall Multiplier</description>
				<number>%P_NWP_MULT%</number>
			</attribute>
			<attribute id="SCEN_CATCH_P_NWP_SELECT" name="Source">
				<description>NWP Rainfall Source</description>
				<text>%P_NWP_SELECT%</text>
			</attribute>
			<attribute id="SCEN_CATCH_P_NWP_MULT" name="Multiplier">
				<description>NWP Rainfall Multiplier</description>
				<number>%P_NWP_MULT%</number>
			</attribute>
		</csvFile>
	</locationSet>
	<locationSet id="URBS_reservoirs">
		<locationSetId>URBS_fcst_sites</locationSetId>
		<constraints>
			<attributeExists id="VBFNAME_URBS"/>
		</constraints>
	</locationSet>
	<locationSet id="URBS_reservoirs_controlled">
		<locationSetId>URBS_reservoirs</locationSetId>
		<constraints>
			<attributeExists id="QRELEASE_URBSID"/>
		</constraints>
	</locationSet>
	<locationSet id="URBS_matching">
		<sortingLocationAttributeId>MATCHING_ORDER</sortingLocationAttributeId>
		<csvFile>
			<file>ROWS_urbs_matching</file>
			<id>%URBS_SUBCATCHID%_%ROWS_MATCHID%</id>
			<name>%URBS_MATCHID%</name>
			<x>%LONG%</x>
			<y>%LAT%</y>
			<attribute id="URBS_SUBCATCHID" name="Urbs sub catchment">
				<text>%URBS_SUBCATCHID%</text>
			</attribute>
			<attribute id="URBS_MATCHID" name="Urbs name">
				<text>%URBS_MATCHID%</text>
			</attribute>
			<attribute id="ROWS_MATCHID" name="ROWS site id">
				<text>%ROWS_MATCHID%</text>
			</attribute>
			<attribute id="INCLUDE_MATCH_IN_URBS" name="Match station">
				<boolean>%INCLUDE_MATCH_IN_URBS%</boolean>
			</attribute>
			<attribute id="MATCHING_ORDER" name="Matching display order">
				<number>%MATCHING_ORDER%</number>
			</attribute>
			<attribute id="MATCHING_ADVICE" name="Matching advice">
				<text>%MATCHING_ADVICE%</text>
			</attribute>
		</csvFile>
	</locationSet>
	<locationSet id="URBS_subcatchments">
		<sortingLocationAttributeId>SC_ORDER</sortingLocationAttributeId>
		<csvFile>
			<file>ROWS_urbs_subcatchments</file>
			<geoDatum>GDA94</geoDatum>
			<id>%SUBCATCHID%</id>
			<name>%SUBCATCHID%</name>
			<description>%URBS_SUBCATCHMENT_NAME%</description>
			<x>%X_CENTROID%</x>
			<y>%Y_CENTROID%</y>
			<relation id="catchment">
				<relatedLocationId>%CATCHMENT%</relatedLocationId>
			</relation>
			<attribute id="URBS_SUBCATCHMENT_ID">
				<description>ROWS URBS sub-catchment identifier</description>
				<text>%SUBCATCHID%</text>
			</attribute>
			<attribute id="URBS_CATCHMENT" name="URBS catchment">
				<description>URBS catchment</description>
				<text>%CATCHMENT%</text>
			</attribute>
			<attribute id="SC_ORDER" name="Sub-catchment order">
				<description>Sub-catchment order</description>
				<number>%SC_ORDER%</number>
			</attribute>
			<attributeFile>
				<csvFile>ROWS_Rainfall_Subcatchments.csv</csvFile>
				<id>%URBS_SUBCATCHMENT_ID%</id>
				<attribute id="SC_P_OBS_SELECT" name="Source">
					<description>Observed Rainfall Source</description>
					<text>%SC_P_OBS_SELECT%</text>
				</attribute>
				<attribute id="SC_P_OBS_MULT" name="Multiplier">
					<description>Observed Rainfall Multiplier</description>
					<number>%SC_P_OBS_MULT%</number>
				</attribute>
				<attribute id="SC_P_MAN_SELECT" name="Type">
					<description>Nowcast Rainfall Source</description>
					<text>%SC_P_MAN_SELECT%</text>
				</attribute>
				<attribute id="SC_P_MAN_DEPTH" name="Depth (mm)">
					<description>Forecaster Rainfall Depth</description>
					<number>%SC_P_MAN_DEPTH%</number>
				</attribute>
				<attribute id="SC_P_MAN_DELAY" name="Delay (hr)">
					<description>Delay of Forecast Rainfall</description>
					<number>%SC_P_MAN_DELAY%</number>
				</attribute>
				<attribute id="SC_P_MAN_MATCH" name="Matching Period (hr)">
					<description>Matching Period of Observed Rainfall</description>
					<number>%SC_P_MAN_MATCH%</number>
				</attribute>
				<attribute id="SC_P_MAN_LENGTH" name="Duration (hr)">
					<description>Forecaster scenario Length</description>
					<number>%SC_P_MAN_LENGTH%</number>
				</attribute>
				<attribute id="SC_P_MAN_FACTOR" name="Recession Factor">
					<description>Factor to use in Recession and Persistence</description>
					<number>%SC_P_MAN_FACTOR%</number>
				</attribute>
				<attribute id="SC_P_MAN_OFFSET" name="T0 Offset (hr)">
					<description>Offset from T0 in model time steps</description>
					<number>%SC_P_MAN_OFFSET%</number>
				</attribute>
				<attribute id="SCEN_SC_P_OBS_SELECT" name="Source">
					<description>Observed Rainfall Source</description>
					<text>%SCEN_SC_P_OBS_SELECT%</text>
				</attribute>
				<attribute id="SCEN_SC_P_OBS_MULT" name="Multiplier">
					<description>Observed Rainfall Multiplier</description>
					<number>%SCEN_SC_P_OBS_MULT%</number>
				</attribute>
				<attribute id="SCEN_SC_P_MAN_SELECT" name="Type">
					<description>Nowcast Rainfall Source</description>
					<text>%SCEN_SC_P_MAN_SELECT%</text>
				</attribute>
				<attribute id="SCEN_SC_P_MAN_DEPTH" name="Depth (mm)">
					<description>Forecaster Rainfall Depth</description>
					<number>%SCEN_SC_P_MAN_DEPTH%</number>
				</attribute>
				<attribute id="SCEN_SC_P_MAN_DELAY" name="Delay (hr)">
					<description>Delay of Forecast Rainfall</description>
					<number>%SCEN_SC_P_MAN_DELAY%</number>
				</attribute>
				<attribute id="SCEN_SC_P_MAN_MATCH" name="Matching Period (hr)">
					<description>Matching Period of Observed Rainfall</description>
					<number>%SCEN_SC_P_MAN_MATCH%</number>
				</attribute>
				<attribute id="SCEN_SC_P_MAN_LENGTH" name="Duration (hr)">
					<description>Forecaster scenario Length</description>
					<number>%SCEN_SC_P_MAN_LENGTH%</number>
				</attribute>
				<attribute id="SCEN_SC_P_MAN_FACTOR" name="Recession Factor">
					<description>Factor to use in Recession and Persistence</description>
					<number>%SCEN_SC_P_MAN_FACTOR%</number>
				</attribute>
				<attribute id="SCEN_SC_P_MAN_OFFSET" name="T0 Offset (hr)">
					<description>Offset from T0 in model time steps</description>
					<number>%SCEN_SC_P_MAN_OFFSET%</number>
				</attribute>
			</attributeFile>
			<attributeFile>
				<csvFile>ROWS_urbs_parameters.csv</csvFile>
				<id>%URBS_SUBCATCHMENT_ID%</id>
				<attribute id="URBS_SUBCATCHMENT" name="URBS sub catchment">
					<description>URBS sub catchment name</description>
					<text>%URBS_SUBCATCHMENT%</text>
				</attribute>
				<attribute id="URBS_SUBCATCHMENT_NAME" name="URBS sub catchment (concatenated name)">
					<description>URBS sub catchment name</description>
					<text>%URBS_SUBCATCHMENT_NAME%</text>
				</attribute>
				<attribute id="URBS_TIMESTEP" name="URBS timestep (h)">
					<description>[URBS_TIMESTEP] URBS timestep (h)</description>
					<number>%URBS_TIMESTEP_IN_HRS%</number>
				</attribute>
				<attribute id="URBS_LOSSTYPE" name="URBS Loss Model Type">
					<description>[URBS_LOSSTYPE] The type of continuing loss model used: ILCL is for a loss rate independent of rainfall and ILPR is for a runoff factor.</description>
					<text>%URBS_LOSSTYPE%</text>
				</attribute>
				<attribute id="URBS_IL" name="Initial Loss (mm)">
					<description>[URBS_IL] Initial rainfall loss.</description>
					<number>%URBS_IL%</number>
				</attribute>
				<attribute id="URBS_CL" name="Continuing Loss (mm/hr)">
					<description>[URBS_CL] Continuing loss rate (ILCL model only).</description>
					<number>%URBS_CL%</number>
				</attribute>
				<attribute id="URBS_PR" name="Proportional Runoff (fraction)">
					<description>[URBS_PR] Runoff proportion (ILPR model only).</description>
					<number>%URBS_PR%</number>
				</attribute>
				<attribute id="URBS_IF" name="Infiltration Capacity (mm)">
					<description>[URBS_IF] Maximum cumulative infiltration until losses become 0. </description>
					<number>%URBS_IF%</number>
				</attribute>
				<attribute id="URBS_ALPHA" name="Alpha">
					<description>[URBS_ALPHA] Channel routing lag. Range: 0 to 0.08</description>
					<number>%URBS_ALPHA%</number>
				</attribute>
				<attribute id="URBS_BETA" name="Beta">
					<description>[URBS_BETA] Catchment routing lag (1-9). Range: 0 to 0.77</description>
