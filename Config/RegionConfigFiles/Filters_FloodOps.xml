	<!--Flood Operations sites (organised by catchment)-->
	<filter id="Catchment_FloodOps" name="Structures">
		<child foreignKey="Catchment_FloodOps_Dartmouth"/>
		<child foreignKey="Catchment_FloodOps_Hume"/>
		<child foreignKey="Catchment_FloodOps_Yarrawonga"/>
	</filter>
	<filter id="Catchment_FloodOps_Dartmouth" name="Dartmouth">
		<validationIconsVisible>true</validationIconsVisible>
		<mapExtentId>Upper Murray</mapExtentId>
		<timeSeriesSetsId>Flood_Ops</timeSeriesSetsId>
		<locationConstraints>
			<attributeTextContains id="FLOOD_OPS" contains="Dartmouth"/>
		</locationConstraints>
	</filter>
	<filter id="Catchment_FloodOps_Hume" name="Hume">
		<validationIconsVisible>true</validationIconsVisible>
		<mapExtentId>Upper Murray</mapExtentId>
		<timeSeriesSetsId>Flood_Ops</timeSeriesSetsId>
		<locationConstraints>
			<attributeTextContains id="FLOOD_OPS" contains="Hume"/>
		</locationConstraints>
	</filter>
	<filter id="Catchment_FloodOps_Yarrawonga" name="Yarrawonga">
		<validationIconsVisible>true</validationIconsVisible>
		<mapExtentId>Upper Murray</mapExtentId>
		<timeSeriesSetsId>Flood_Ops</timeSeriesSetsId>
		<locationConstraints>
			<attributeTextContains id="FLOOD_OPS" contains="Yarrawonga"/>
		</locationConstraints>
	</filter>
	<!--Flood Operations sites (organised by parameter)-->
	<filter id="Parameter_FloodOps" name="Parameters">
		<child foreignKey="Parameter_FloodOps_Hobs"/>
		<child foreignKey="Parameter_FloodOps_Pobs"/>
		<child foreignKey="Parameter_FloodOps_Qobs"/>
		<child foreignKey="Parameter_FloodOps_Vobs"/>
	</filter>
	<filter id="Parameter_FloodOps_Hobs" name="Water level">
		<validationIconsVisible>true</validationIconsVisible>
		<mapExtentId>Upper Murray</mapExtentId>
		<timeSeriesSetsId>Flood_Ops_Hobs</timeSeriesSetsId>
	</filter>
	<filter id="Parameter_FloodOps_Pobs" name="Precipitation">
		<validationIconsVisible>true</validationIconsVisible>
		<mapExtentId>Upper Murray</mapExtentId>
		<timeSeriesSetsId>Flood_Ops_Pobs</timeSeriesSetsId>
	</filter>
	<filter id="Parameter_FloodOps_Qobs" name="Flow">
		<validationIconsVisible>true</validationIconsVisible>
		<mapExtentId>Upper Murray</mapExtentId>
		<timeSeriesSetsId>Flood_Ops_Qobs</timeSeriesSetsId>
	</filter>
	<filter id="Parameter_FloodOps_Vobs" name="Volume">
		<validationIconsVisible>true</validationIconsVisible>
		<mapExtentId>Upper Murray</mapExtentId>
		<timeSeriesSetsId>Flood_Ops_Vobs</timeSeriesSetsId>
	</filter>
