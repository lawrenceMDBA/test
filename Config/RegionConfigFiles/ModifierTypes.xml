						<attributeId>SCEN_CATCH_P_NWP_SELECT</attributeId>
						<attributeId>SCEN_CATCH_P_NWP_MULT</attributeId>
					</tableLayout>
				</panelLayout>
				<summaryFunction>Grid=@SCEN_CATCH_P_NWP_SELECT@</summaryFunction>
			</modifiableGroup>
			<modifiableGroup name="SubCatchments">
